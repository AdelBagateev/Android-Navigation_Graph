<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/homeFragment">
    <include app:graph = "@navigation/navigate_graph"/>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.navigation_graph.fragments.for_bnv.HomeFragment"
        android:label="HomeFragment"
        tools:layout = "@layout/fragment_home"
        >
    </fragment>
    <fragment
        android:id="@+id/likesFragment"
        android:name="com.example.navigation_graph.fragments.for_bnv.LikesFragment"
        android:label="LikesFragment" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.navigation_graph.fragments.for_bnv.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.navigation_graph.fragments.for_bnv.SearchFragment"
        android:label="SearchFragment" />

</navigation>